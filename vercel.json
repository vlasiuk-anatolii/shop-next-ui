{
    "rewrites": [
      {
        "source": "/auth/:path*",
        "destination": "http://shop-backend-env.eba-kpsmmad2.eu-north-1.elasticbeanstalk.com/auth/:path*"
      },
      {
        "source": "/users/:path*",
        "destination": "http://shop-backend-env.eba-kpsmmad2.eu-north-1.elasticbeanstalk.com/users/:path*"
      },
      {
        "source": "/products",
        "destination": "http://shop-backend-env.eba-kpsmmad2.eu-north-1.elasticbeanstalk.com/products"
      },
      {
        "source": "/products/:path*",
        "destination": "http://shop-backend-env.eba-kpsmmad2.eu-north-1.elasticbeanstalk.com/products/:path*"
      },
      {
        "source": "/checkout/:path*",
        "destination": "http://shop-backend-env.eba-kpsmmad2.eu-north-1.elasticbeanstalk.com/checkout/:path*"
      },
      {
        "source": "/socket.io/:path*",
        "destination": "http://shop-backend-env.eba-kpsmmad2.eu-north-1.elasticbeanstalk.com/socket.io/:path*"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Content-Security-Policy", "value": "upgrade-insecure-requests" },
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept" }
        ]
      }
    ]
  }